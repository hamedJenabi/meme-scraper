<script>
      var clipboard = new Clipboard('.btn', {
        text: function(trigger) {
          if (trigger.id == 'btn-copy-link') {
            return "https://memegen.link/patrick/your_text/goes_here.jpg?preview=true&share=true&watermark=none";
          } else if (trigger.id == 'btn-copy-image') {
            return "https://memegen.link/patrick/your_text/goes_here.jpg?preview=true";
          }
        }
      });
      clipboard.on('success', function(event) {
        $(event.trigger).attr('title', 'Link Copied').tooltip('fixTitle').tooltip('show');
      });
      clipboard.on('error', function(event) {
        console.log(event);
      });
    </script>
